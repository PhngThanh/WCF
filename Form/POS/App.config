  <log4net>
    <appender name="LogBug" type="log4net.Appender.RollingFileAppender">
      <file value="Logs\\" />
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="ERROR" />
      </filter>
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="true" />
      <DatePattern value="yyyy-MMM\\\\dd'-POS-Bug.txt'" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <appender name="LogInfo" type="log4net.Appender.RollingFileAppender">
      <file value="Logs\\" />
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO" />
        <levelMax value="INFO" />
      </filter>
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="true" />
      <DatePattern value="yyyy-MMM\\\\dd'-POS-Info.txt'" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogBug" />
      <appender-ref ref="LogInfo" />
    </root>
  </log4net>

